DEVICES:
    SWITCH = sw1 (0),
    SWITCH = sw2 (0),
    SWITCH = sw3 (1),
    CLOCK = clock1 (5),
    XOR = xor1,
    XOR = xor2,
    DTYPE = d1;

CONNECTIONS:
    sw1 => xor1.I1,
    sw2 => xor1.I2,
    d1.Q => xor2.I1,
    xor1 => xor2.I2,
    xor2 => d1.DATA,
    clock1 => d1.CLK,
    sw3 => d1.SET,
    sw3 => d1.CLEAR;

MONITORS:
    xor1, xor2, d1.Q;
